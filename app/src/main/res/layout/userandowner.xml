<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/log_sign_back"
    tools:context=".UserAndOwner">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="252dp"
        android:layout_height="77dp"
        android:fontFamily="@font/alice"
        android:text="@string/welcome"
        android:textAlignment="center"
        android:textColor="#3049DB"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="88dp"
        android:layout_height="56dp"
        android:fontFamily="@font/alice"
        android:text="@string/to"
        android:textAlignment="center"
        android:textColor="#3049DB"
        android:textSize="36sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="235dp"
        android:layout_height="85dp"
        android:fontFamily="@font/alice"
        android:text="@string/s_park"
        android:textAlignment="center"
        android:textColor="#F44336"
        android:textSize="60sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="194dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="120dp"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="319dp"
        android:onClick="userSend"
        android:background="@drawable/user_start"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/owner_start"
        android:onClick="ownerSend"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="471dp" />
</androidx.constraintlayout.widget.ConstraintLayout>