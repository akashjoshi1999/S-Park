<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AccountActivity">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="171dp"
            android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/gradient_red">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:layout_editor_absoluteX="140dp"
                    tools:layout_editor_absoluteY="8dp"
                    tools:srcCompat="@tools:sample/avatars[0]" />

                <TextView
                    android:id="@+id/text_nameview"
                    android:layout_width="149dp"
                    android:layout_height="28dp"
                    android:text=""
                    android:textAlignment="center"
                    tools:layout_editor_absoluteX="130dp"
                    tools:layout_editor_absoluteY="136dp" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ProgressBar
                android:id="@+id/progressBar4"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="414dp"
                android:layout_height="44dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.568"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="189dp" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="86dp"
                android:layout_height="30dp"
                android:text="Personal"
                android:textColor="#b3b3b3"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="26dp" />

            <TextView
                android:id="@+id/text_username"
                android:layout_width="195dp"
                android:layout_height="26dp"
                android:text=""
                android:textAlignment="textEnd"
                android:textColor="#ff3300"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="26dp" />

            <TextView
                android:id="@+id/text_useremail"
                android:layout_width="195dp"
                android:layout_height="26dp"
                android:text=""
                android:textAlignment="textEnd"
                android:textColor="#ff3300"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="81dp" />

            <TextView
                android:id="@+id/text_userphone"
                android:layout_width="195dp"
                android:layout_height="26dp"
                android:text=""
                android:textAlignment="center"
                android:textColor="#ff3300"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="133dp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="86dp"
                android:layout_height="30dp"
                android:text="Vehicles"
                android:textColor="#b3b3b3"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="264dp" />

            <TextView
                android:id="@+id/text_uservehicle"
                android:layout_width="195dp"
                android:layout_height="29dp"
                android:onClick="addVehicle"
                android:text="Add Vehicle"
                android:textAlignment="textEnd"
                android:textColor="#ff3300"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="265dp" />

            <ProgressBar
                android:id="@+id/progressBar3"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="405dp"
                android:layout_height="37dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.568"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="328dp" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="86dp"
                android:layout_height="30dp"
                android:text="Other"
                android:textColor="#b3b3b3"
                tools:layout_editor_absoluteX="29dp"
                tools:layout_editor_absoluteY="395dp" />

            <TextView
                android:id="@+id/text_userDeactivate"
                android:layout_width="195dp"
                android:layout_height="29dp"
                android:onClick="accountDeactivate"
                android:text="Add Vehicle"
                android:textAlignment="textEnd"
                android:textColor="#ff3300"
                tools:layout_editor_absoluteX="212dp"
                tools:layout_editor_absoluteY="265dp" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>